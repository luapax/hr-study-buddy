{
    "extends": [
        "react-app",
        "react-app/jest",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "singleQuote": true,
                "semi": true,
                "trailingComma": "es5",
                "htmlWhitespaceSensitivity": "ignore",
                "endOfLine": "auto"
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@babel/eslint-parser"
}